install-nobase_distconfSCRIPTS: $(nobase_distconf_SCRIPTS)
	@$(NORMAL_INSTALL)
	test -z "$(distconfdir)" || $(MKDIR_P) "$(DESTDIR)$(distconfdir)"
	@$(am__vpath_adj_setup) \
	list='$(nobase_distconf_SCRIPTS)'; for p in $$list; do \
	  $(am__vpath_adj) p=$$f; \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    f=`echo "$$p" | sed 's|[^/]*$$||'`"$$f"; \
	    echo " $(nobase_distconfSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(distconfdir)/$$f'"; \
	    $(nobase_distconfSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(distconfdir)/$$f"; \
	  else :; fi; \
	done

uninstall-nobase_distconfSCRIPTS:
	@$(NORMAL_UNINSTALL)
	@$(am__vpath_adj_setup) \
	list='$(nobase_distconf_SCRIPTS)'; for p in $$list; do \
	  $(am__vpath_adj) p=$$f; \
	  f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	  f=`echo "$$p" | sed 's|[^/]*$$||'`"$$f"; \
	  echo " rm -f '$(DESTDIR)$(distconfdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(distconfdir)/$$f"; \
	done

